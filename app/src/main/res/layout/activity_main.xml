<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/button4"
        style="@style/Widget.AppCompat.Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/teal_700"
        android:backgroundTintMode="add"
        android:text="Пройти тест"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#FFEB3B"
        android:textSize="18"
        android:textStyle="bold"
        app:iconTint="#FFEB3B"
        tools:layout_editor_absoluteX="126dp"
        tools:layout_editor_absoluteY="405dp" />

    <Button
        android:id="@+id/button3"
        style="@style/Widget.MaterialComponents.TimePicker.Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/teal_700"
        android:backgroundTintMode="add"
        android:text="Уже имеется аккаунт"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="#71716E"
        android:textSize="18"
        android:textStyle="bold"
        app:iconTint="#FFEB3B"
        tools:layout_editor_absoluteX="111dp"
        tools:layout_editor_absoluteY="644dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Тест личности"
        android:textSize="50"
        android:textStyle="bold"
        android:visibility="visible"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="172dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-condensed"
        android:text="MBTI"
        android:textSize="80"
        android:textStyle="bold"
        android:visibility="visible"
        tools:layout_editor_absoluteX="120dp"
        tools:layout_editor_absoluteY="239dp" />

    <Button
        android:id="@+id/button"
        style="@style/Widget.AppCompat.Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/teal_700"
        android:backgroundTintMode="add"
        android:text="Регистрация"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#FFEB3B"
        android:textSize="18"
        android:textStyle="bold"
        app:iconTint="#FFEB3B"
        tools:layout_editor_absoluteX="126dp"
        tools:layout_editor_absoluteY="588dp" />

</androidx.constraintlayout.widget.ConstraintLayout>